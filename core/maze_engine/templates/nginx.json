{
  "name": "nginx",
  "version": "1.22.1",
  "fingerprint_id": "nginx_1221_ubuntu",
  "headers": {
    "Server": "nginx/1.22.1",
    "Connection": "keep-alive",
    "Vary": "Accept-Encoding",
    "X-Content-Type-Options": "nosniff",
    "X-Frame-Options": "DENY",
    "X-XSS-Protection": "1; mode=block"
  },
  "error_pages": {
    "404": {
      "title": "404 Not Found",
      "body": "<html>\r\n<head><title>404 Not Found</title></head>\r\n<body>\r\n<center><h1>404 Not Found</h1></center>\r\n<hr><center>nginx/1.22.1</center>\r\n</body>\r\n</html>",
      "content_type": "text/html"
    },
    "403": {
      "title": "403 Forbidden",
      "body": "<html>\r\n<head><title>403 Forbidden</title></head>\r\n<body>\r\n<center><h1>403 Forbidden</h1></center>\r\n<hr><center>nginx/1.22.1</center>\r\n</body>\r\n</html>",
      "content_type": "text/html"
    },
    "502": {
      "title": "502 Bad Gateway",
      "body": "<html>\r\n<head><title>502 Bad Gateway</title></head>\r\n<body>\r\n<center><h1>502 Bad Gateway</h1></center>\r\n<hr><center>nginx/1.22.1</center>\r\n</body>\r\n</html>",
      "content_type": "text/html"
    }
  },
  "behavioral_patterns": {
    "response_timing": {
      "min_ms": 25,
      "max_ms": 120,
      "distribution": "exponential",
      "jitter_factor": 0.1
    },
    "keep_alive": {
      "timeout": 75,
      "max_requests": 1000
    },
    "compression": {
      "gzip": true,
      "deflate": false,
      "br": true,
      "min_size": 1000,
      "level": 6
    },
    "worker_processes": 4,
    "worker_connections": 1024
  },
  "vulnerability_simulation": {
    "cve_2013_4547": {
      "enabled": false,
      "paths": ["/admin.php", "/config.php"],
      "response": "404 Not Found"
    },
    "nginx_status": {
      "enabled": true,
      "path": "/nginx_status",
      "response_template": "nginx_status_template"
    },
    "alias_traversal": {
      "enabled": true,
      "detection_patterns": ["../", "..%2f"],
      "response": "400 Bad Request"
    }
  },
  "ssl_config": {
    "protocols": ["TLSv1.2", "TLSv1.3"],
    "ciphers": "ECDHE-RSA-AES256-GCM-SHA384:ECDHE-RSA-CHACHA20-POLY1305:ECDHE-RSA-AES128-GCM-SHA256",
    "certificate_info": {
      "issuer": "Cloudflare Inc ECC CA-3",
      "subject": "CN={{host}}",
      "san": ["{{host}}", "*.{{host}}"],
      "valid_from": "{{cert_start}}",
      "valid_to": "{{cert_end}}",
      "signature_algorithm": "ecdsa-with-SHA256"
    },
    "hsts": {
      "enabled": true,
      "max_age": 63072000,
      "include_subdomains": true,
      "preload": true
    },
    "ocsp_stapling": true
  },
  "modules": [
    "http_ssl_module",
    "http_v2_module",
    "http_realip_module",
    "http_addition_module",
    "http_geoip_module",
    "http_gunzip_module",
    "http_gzip_static_module"
  ],
  "log_format": "{{client_ip}} - {{remote_user}} [{{timestamp}}] \"{{request}}\" {{status}} {{body_bytes_sent}} \"{{http_referer}}\" \"{{http_user_agent}}\"",
  "reconnaissance_responses": {
    "nmap_scan": {
      "tcp_window_size": 29200,
      "tcp_options": "mss 1460,sackOK,timestamp,nop,wscale 6",
      "service_banner": "nginx 1.22.1"
    },
    "nikto_scan": {
      "robots_txt": "User-agent: *\nDisallow: /admin\nDisallow: /.git",
      "server_info": "nginx/1.22.1"
    },
    "dirb_scan": {
      "common_paths": ["/admin", "/api", "/backup", "/config"],
      "response_codes": [404, 403, 301]
    }
  },
  "rate_limiting": {
    "enabled": true,
    "requests_per_second": 10,
    "burst": 20,
    "delay": 5
  }
}
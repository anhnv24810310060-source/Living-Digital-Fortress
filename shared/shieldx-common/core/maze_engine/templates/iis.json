{
  "name": "iis",
  "version": "10.0",
  "fingerprint_id": "iis_10_windows_server_2019",
  "headers": {
    "Server": "Microsoft-IIS/10.0",
    "X-Powered-By": "ASP.NET",
    "X-AspNet-Version": "4.0.30319",
    "X-AspNetMvc-Version": "5.2",
    "X-Content-Type-Options": "nosniff",
    "X-Frame-Options": "SAMEORIGIN"
  },
  "error_pages": {
    "404": {
      "title": "404 - File or directory not found.",
      "body": "<!DOCTYPE html PUBLIC \"-//W3C//DTD XHTML 1.0 Strict//EN\" \"http://www.w3.org/TR/xhtml1/DTD/xhtml1-strict.dtd\">\r\n<html xmlns=\"http://www.w3.org/1999/xhtml\">\r\n<head>\r\n<title>IIS Windows Server</title>\r\n<style type=\"text/css\">\r\nbody{margin:0;font-size:.7em;font-family:Verdana,Arial,Helvetica,sans-serif;background:#EEEEEE;}\r\n</style>\r\n</head>\r\n<body>\r\n<div id=\"header\"><h1>Server Error</h1></div>\r\n<div id=\"content\">\r\n<div class=\"content-container\"><fieldset>\r\n<h2>404 - File or directory not found.</h2>\r\n<h3>The resource you are looking for might have been removed, had its name changed, or is temporarily unavailable.</h3>\r\n</fieldset></div>\r\n</div>\r\n</body>\r\n</html>",
      "content_type": "text/html; charset=utf-8"
    },
    "403": {
      "title": "403 - Forbidden: Access is denied.",
      "body": "<!DOCTYPE html PUBLIC \"-//W3C//DTD XHTML 1.0 Strict//EN\" \"http://www.w3.org/TR/xhtml1/DTD/xhtml1-strict.dtd\">\r\n<html xmlns=\"http://www.w3.org/1999/xhtml\">\r\n<head>\r\n<title>IIS Windows Server</title>\r\n</head>\r\n<body>\r\n<div id=\"header\"><h1>Server Error</h1></div>\r\n<div id=\"content\">\r\n<h2>403 - Forbidden: Access is denied.</h2>\r\n<h3>You do not have permission to view this directory or page using the credentials that you supplied.</h3>\r\n</div>\r\n</body>\r\n</html>",
      "content_type": "text/html; charset=utf-8"
    },
    "500": {
      "title": "500 - Internal server error.",
      "body": "<!DOCTYPE html PUBLIC \"-//W3C//DTD XHTML 1.0 Strict//EN\" \"http://www.w3.org/TR/xhtml1/DTD/xhtml1-strict.dtd\">\r\n<html xmlns=\"http://www.w3.org/1999/xhtml\">\r\n<head>\r\n<title>IIS Windows Server</title>\r\n</head>\r\n<body>\r\n<div id=\"header\"><h1>Server Error in Application</h1></div>\r\n<div id=\"content\">\r\n<h2>500 - Internal server error.</h2>\r\n<h3>There is a problem with the resource you are looking for, and it cannot be displayed.</h3>\r\n</div>\r\n</body>\r\n</html>",
      "content_type": "text/html; charset=utf-8"
    }
  },
  "behavioral_patterns": {
    "response_timing": {
      "min_ms": 60,
      "max_ms": 250,
      "distribution": "normal",
      "jitter_factor": 0.2
    },
    "keep_alive": {
      "timeout": 120,
      "max_requests": 100
    },
    "compression": {
      "gzip": true,
      "deflate": true,
      "br": false
    },
    "session_state": {
      "mode": "InProc",
      "timeout": 20,
      "cookie_name": "ASP.NET_SessionId"
    }
  },
  "vulnerability_simulation": {
    "cve_2017_7269": {
      "enabled": false,
      "description": "WebDAV ScStoragePathFromUrl buffer overflow",
      "response": "500 Internal Server Error"
    },
    "aspnet_trace": {
      "enabled": true,
      "path": "/trace.axd",
      "response": "403 Forbidden"
    },
    "directory_browsing": {
      "enabled": false,
      "response": "403 Forbidden"
    },
    "debug_mode": {
      "enabled": false,
      "detection_patterns": ["compilation debug=\"true\""],
      "response": "detailed_error_page"
    }
  },
  "ssl_config": {
    "protocols": ["TLS 1.2", "TLS 1.3"],
    "ciphers": "TLS_ECDHE_RSA_WITH_AES_256_GCM_SHA384:TLS_ECDHE_RSA_WITH_AES_128_GCM_SHA256",
    "certificate_info": {
      "issuer": "DigiCert SHA2 Secure Server CA",
      "subject": "CN={{host}}",
      "san": ["{{host}}"],
      "valid_from": "{{cert_start}}",
      "valid_to": "{{cert_end}}",
      "signature_algorithm": "sha256RSA"
    },
    "hsts": {
      "enabled": false,
      "max_age": 0
    }
  },
  "modules": [
    "DefaultDocumentModule",
    "DirectoryListingModule",
    "StaticFileModule",
    "AnonymousAuthenticationModule",
    "WindowsAuthenticationModule",
    "ManagedEngine",
    "AspNetCoreModule"
  ],
  "log_format": "{{date}} {{time}} {{s-sitename}} {{s-computername}} {{s-ip}} {{cs-method}} {{cs-uri-stem}} {{cs-uri-query}} {{s-port}} {{cs-username}} {{c-ip}} {{cs-version}} {{cs(User-Agent)}} {{cs(Cookie)}} {{cs(Referer)}} {{cs-host}} {{sc-status}} {{sc-substatus}} {{sc-win32-status}} {{sc-bytes}} {{cs-bytes}} {{time-taken}}",
  "reconnaissance_responses": {
    "nmap_scan": {
      "tcp_window_size": 65535,
      "tcp_options": "mss 1460,nop,wscale 8,nop,nop,sackOK",
      "service_banner": "Microsoft IIS httpd 10.0"
    },
    "nikto_scan": {
      "robots_txt": "User-agent: *\nDisallow: /admin/\nDisallow: /App_Data/",
      "server_info": "Microsoft-IIS/10.0 ASP.NET"
    },
    "dirb_scan": {
      "common_paths": ["/admin", "/App_Data", "/bin", "/web.config"],
      "response_codes": [404, 403, 401]
    }
  },
  "aspnet_config": {
    "framework_version": "4.8",
    "compilation_debug": false,
    "custom_errors": "RemoteOnly",
    "authentication_mode": "Windows",
    "session_timeout": 20,
    "max_request_length": 4096,
    "execution_timeout": 110
  },
  "windows_features": [
    "IIS-WebServerRole",
    "IIS-WebServer",
    "IIS-CommonHttpFeatures",
    "IIS-HttpErrors",
    "IIS-HttpRedirect",
    "IIS-ApplicationDevelopment",
    "IIS-NetFxExtensibility45",
    "IIS-ISAPIExtensions",
    "IIS-ISAPIFilter",
    "IIS-ASPNET45"
  ]
}